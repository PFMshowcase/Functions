{
	"compilerOptions": {
		"module": "NodeNext",
		"noImplicitReturns": true,
		"noImplicitAny": true,
		"noUnusedLocals": false,
		"outDir": "lib",
		"rootDir": "src",
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	},
	"compileOnSave": true,
	"include": ["src", ".d.ts", "**/**/*.png"],
	"allowJs": true
}
